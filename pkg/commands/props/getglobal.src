use uo;

program textcmd_getglobal( who, propname )
	var res := GetGlobalProperty( propname );

	if (res != error)
		SendSysMessage( who, "Property "+propname+": "+res );
	else
		SendSysMessage( who, "Error getting property: "+res.errortext );
	endif

	return 1;
endprogram