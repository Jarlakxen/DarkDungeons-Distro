 *
 * Purpose
 * This script allows the scripter to run code when a character is deleted.
 *
 */

use uo;
use os;


program core_onDelete(mobile)
	SysLog("Deleting character "+mobile.name+" ("+mobile.serial+") from account ["+mobile.acctname+"]");

	return 1;
endprogram
