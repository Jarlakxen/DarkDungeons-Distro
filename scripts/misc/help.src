use uo;
use os;

program Help(mobile)
	var script := Start_Script(":help:helpSystem/helpSystem", mobile);
	if ( script.errortext )
		SendSysMessage(mobile, "Error starting help system ->"+script.errortext);
		SysLog("Error starting help system->"+script.errortext);
		return 0;
	endif

	return 1;
endprogram
